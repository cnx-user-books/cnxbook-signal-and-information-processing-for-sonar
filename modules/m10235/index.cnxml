			<m:msub>
			  <m:mi>t</m:mi>
			  <m:mn>2</m:mn>
			</m:msub>
		      </m:ci>
		    </m:apply>
		    <m:apply>
		      <m:minus/>
		      <m:apply>
			<m:arccos/>
			<m:ci>
			  <m:msub>
			    <m:mi>x</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub>
			</m:ci>
		      </m:apply>
		      <m:apply>
			<m:times/>
			<m:cn>2</m:cn>
			<m:pi/>
			<m:ci>
			  <m:msub>
			    <m:mi>f</m:mi>
			    <m:mn>0</m:mn>
			  </m:msub>
			</m:ci>
			<m:ci>
			  <m:msub>
			    <m:mi>t</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub>
			</m:ci>
		      </m:apply>
		    </m:apply>
		  </m:apply>
                </m:apply>
                <m:apply>
                  <m:cos/>
		  <m:apply>
		    <m:plus/>
		    <m:apply>
		      <m:times/>
		      <m:cn>2</m:cn>
		      <m:pi/>
		      <m:ci>
			<m:msub>
			  <m:mi>f</m:mi>
			  <m:mn>0</m:mn>
			</m:msub>
		      </m:ci>
		      <m:apply>
			<m:minus/>
			<m:ci>
			  <m:msub>
			    <m:mi>t</m:mi>
			    <m:mn>2</m:mn>
			  </m:msub>
			</m:ci>
			<m:ci>
			  <m:msub>
			    <m:mi>t</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub>
			</m:ci>
		      </m:apply>
		    </m:apply>
		    <m:apply>
		      <m:arccos/>
		      <m:ci>
			<m:msub>
			  <m:mi>x</m:mi>
			  <m:mn>1</m:mn>
			</m:msub>
		      </m:ci>
		    </m:apply>
		  </m:apply>
                </m:apply>
	      </m:apply>
	    </m:math>
	  </equation>

	</para>

	<figure id="fig3">
	  <media id="idm12587840" alt=""><image src="../../media/Figure3-3b.png" mime-type="image/png"/></media>
	</figure>

	<para id="para6">
	  <equation id="eq14">
	    <m:math display="block">
	      <m:apply>
		<m:eq/>
                <m:apply>
                  <m:ci type="fn">
                    <m:msub>
                      <m:mi>F</m:mi>
                      <m:mrow>
                        <m:msub>
                          <m:mi>X</m:mi>
                          <m:msub>
                            <m:mi>t</m:mi>
                            <m:mn>2</m:mn>
                          </m:msub>
                        </m:msub>
                        <m:mo>,</m:mo>
                        <m:msub>
                          <m:mi>X</m:mi>
                          <m:msub>
                            <m:mi>t</m:mi>
                            <m:mn>1</m:mn>
                          </m:msub>
                        </m:msub>
                      </m:mrow>
                    </m:msub>
                  </m:ci>
                  <m:ci>
                    <m:msub>
                      <m:mi>b</m:mi>
                      <m:mn>2</m:mn>
                    </m:msub>
                  </m:ci>
                  <m:ci>
                    <m:msub>
                      <m:mi>b</m:mi>
                      <m:mn>1</m:mn>
                    </m:msub>
                  </m:ci>
                </m:apply>
                <m:apply>
                  <m:int/>
		  <m:bvar>
		    <m:ci>
		      <m:msub>
			<m:mi>x</m:mi>
			<m:mn>1</m:mn>
		      </m:msub>
		    </m:ci>
		  </m:bvar>
		  <m:lowlimit>
		    <m:apply>
		      <m:minus/>
		      <m:infinity/>
		    </m:apply>
		  </m:lowlimit>
		  <m:uplimit>
		    <m:ci>
		      <m:msub>
			<m:mi>b</m:mi>
			<m:mn>1</m:mn>
		      </m:msub>
		    </m:ci>
		  </m:uplimit>
		  <m:apply>
		    <m:times/>
		      <m:apply>
			<m:ci type="fn">
			  <m:msub>
			    <m:mi>f</m:mi>
			    <m:msub>
			      <m:mi>X</m:mi>
			      <m:msub>
				<m:mi>t</m:mi>
				<m:mn>1</m:mn>
			      </m:msub>
			    </m:msub>
			  </m:msub></m:ci>
			<m:ci>
			  <m:msub>
			    <m:mi>x</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub>
			</m:ci>
		      </m:apply>
		    <m:apply>
			<m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#probability"/>
			<m:condition>
			  <m:apply>
			    <m:eq/>
			    <m:ci>
			      <m:msub>
				<m:mi>X</m:mi>
				<m:msub>
				  <m:mi>t</m:mi>
				  <m:mn>1</m:mn>
				</m:msub>
			      </m:msub>
			    </m:ci>
			    <m:ci>
			      <m:msub>
				<m:mi>x</m:mi>
				<m:mn>1</m:mn>
			      </m:msub>
			    </m:ci>
			  </m:apply>
			</m:condition>
			<m:apply>
			  <m:leq/>
			  <m:ci>
			    <m:msub>
			      <m:mi>X</m:mi>
			      <m:msub>
				<m:mi>t</m:mi>
				<m:mn>2</m:mn>
			      </m:msub>
			    </m:msub>
			  </m:ci>
			  <m:ci>
			    <m:msub>
			      <m:mi>b</m:mi>
			      <m:mn>2</m:mn>
			    </m:msub>
			  </m:ci>
			</m:apply>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </equation>
	  
	  Note that this is only a function of
	  
	  <m:math display="inline">
	    <m:apply>
	      <m:minus/>
	        <m:ci>
	          <m:msub>
	            <m:mi>t</m:mi>
	            <m:mn>2</m:mn>
	          </m:msub>
	        </m:ci>
	        <m:ci>
	          <m:msub>
	            <m:mi>t</m:mi>
	            <m:mn>1</m:mn>
	          </m:msub>
	        </m:ci>	
	    </m:apply>
	  </m:math>.
	</para>

      </example>

      <example id="example3">
	<para id="para7">
	  Every <m:math><m:ci>T</m:ci></m:math> seconds, a fair coin
	  is tossed.  If heads, then
	  
	  <m:math display="inline">
	    <m:apply>
	      <m:eq/>
              <m:ci>
                <m:msub>
                  <m:mi>X</m:mi>
                  <m:mi>t</m:mi>
                </m:msub>
              </m:ci>
              <m:cn>1</m:cn>
	    </m:apply>
	  </m:math>
	  
	  for
	  
	  <m:math display="inline">
	    <m:apply>
	      <m:leq/>
              <m:apply>
                <m:times/>
		<m:ci>n</m:ci>
		<m:ci>T</m:ci>
              </m:apply>
              <m:apply>
                <m:lt/>
		<m:ci>t</m:ci>
		<m:apply>
		  <m:times/>
		  <m:apply>
		    <m:plus/>
		    <m:ci>n</m:ci>
		    <m:cn>1</m:cn>
		  </m:apply>
		  <m:ci>T</m:ci>
		</m:apply>
              </m:apply>
	    </m:apply>
	  </m:math>.
	  
	  If tails, then
	  <m:math display="inline">
	    <m:apply>
	      <m:eq/>
              <m:ci>
                <m:msub>
                  <m:mi>X</m:mi>
                  <m:mi>t</m:mi>
                </m:msub>
              </m:ci>
		<m:cn>-1</m:cn>
	    </m:apply>
	  </m:math>
	  for
	  <m:math display="inline">
	    <m:apply>
	      <m:leq/>
              <m:apply>
                <m:times/>
		<m:ci>n</m:ci>
		<m:ci>T</m:ci>
              </m:apply>
